#!/bin/bash
if [ -e jre/bin/java ]
then
  JAVA=jre/bin/java
else
  JAVA=java
fi
$JAVA -mx512m -cp bio-formats.jar:commons-httpclient-2.0-rc2.jar:commons-logging.jar:forms-1.0.4.jar:ij.jar:looks-1.2.2.jar:ome-java.jar:visad.jar:visbio.jar:xmlrpc-1.2-b1.jar -Dswing.defaultlaf=com.jgoodies.plaf.plastic.Plastic3DLookAndFeel loci.visbio.ome.ImageUploader $*
